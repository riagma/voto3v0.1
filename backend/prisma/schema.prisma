generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Usuario {
  idUnico      String   @id @default(uuid())
  nombre       String   @unique
  nif          String   @unique
  claveServidor String
  claveLocal   String
}

model Eleccion {
  id                String   @id @default(uuid())
  titulo            String
  descripcion       String
  fechaInicio       DateTime
  fechaFin          DateTime
  candidatos        String[]
}
